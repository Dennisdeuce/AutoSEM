modules = ["python-3.11"]

[[ports]]
localPort = 8000
externalPort = 80

[agent]
expertMode = true

[deployment]
deploymentTarget = "autoscale"
run = ["python", "-m", "uvicorn", "main:app", "--host", "0.0.0.0", "--port", "8000"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "AutoSEM API"

[[workflows.workflow]]
name = "AutoSEM API"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python -m uvicorn main:app --host 0.0.0.0 --port 8000"
waitForPort = 8000
